    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.50.0",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
